<div class="min-height-50px">
  <div class="page-header">
    <div class="row">
      <div class="col-md-6 col-sm-12">
        <div class="title">
          <h4>Province</h4>
        </div>
        <nav aria-label="breadcrumb" role="navigation">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="index.html">Province</a></li>
          </ol>
        </nav>
      </div>
    </div>
  </div>
</div>
<div class="pd-20 card-box mb-30">
  <div class="clearfix">
    <div class="pull-left">
      <h4 class="text-blue h4">Detail</h4>
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Province</label>
    <div class="col-sm-12 col-md-10">
      <input class="form-control" type="text" readonly value="<%= province.name %>" >
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Created by</label>
    <div class="col-sm-12 col-md-10">
      <input class="form-control" type="text" readonly value="<%= province.created_by_name %>" >
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Created at</label>
    <div class="col-sm-12 col-md-10">
      <input class="form-control" type="text" readonly value="<%= province.created_at %>" >
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Last updated by</label>
    <div class="col-sm-12 col-md-10">
      <input class="form-control" type="text" readonly value="<%= province.updated_by_name %>" >
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-12 col-md-2 col-form-label">Last updated at</label>
    <div class="col-sm-12 col-md-10">
      <input class="form-control" type="text" readonly value="<%= province.updated_at %>" >
    </div>
  </div>
</div>
<%= render(TableShowComponent.new(
    data: @regencies, 
    model: 'Regency', 
    relation: 'province_id', 
    id: @province.id, 
    search_path: regencies_path)) %>
