<div id="<%= dom_id membership_package %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= membership_package.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= membership_package.description %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price:</strong>
    <%= membership_package.price %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Duration:</strong>
    <%= membership_package.duration %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= membership_package.status %>
  </p>

</div>
