<div id="<%= dom_id village %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= village.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Created by:</strong>
    <%= village.created_by %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Created by name:</strong>
    <%= village.created_by_name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Updated by:</strong>
    <%= village.updated_by %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Updated by name:</strong>
    <%= village.updated_by_name %>
  </p>

</div>
